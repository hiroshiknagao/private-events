<h1><%= @event.title %></h1>
<p><%= @event.description %></p>
<p><%= @event.location %></p>
<p><%= @event.date %></p>
<p><%= @event.start_time %></p>
<p><%= @event.finish_time %></p>
<ul>
  <% @event.attendees.each do |attendee| %>
    <li><%= attendee.name %> (<%= attendee.email %>)</li>
  <% end %>
</ul>
